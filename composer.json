{
    "name": "lsnepomuceno/php-pdf-processor",
    "description": "PDF file parser using native PHP features",
    "type": "library",
    "support": {
        "issues": "https://github.com/lsnepomuceno/php-pdf-processor/issues",
        "source": "https://github.com/lsnepomuceno/php-pdf-processor"
    },
    "keywords": [
        "a1",
        "sign pdf",
        "sign",
        "x509",
        "php sign pdf",
        "php pdf parser",
        "certificate",
        "icp brasil"
    ],
    "authors": [
        {
            "name": "Lucas Nepomuceno",
            "email": "lsn.nepomuceno@gmail.com",
            "homepage": "https://github.com/lsnepomuceno"
        }
    ],
    "require": {
        "php": "^8.1 || ^8.2",
        "ext-iconv": "*",
        "ext-fileinfo": "*",
        "ext-mbstring": "*",
        "ext-xml": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-openssl": "*",
        "ext-zip": "*",
        "ext-tokenizer": "*",
        "ext-gd": "*",
        "ext-simplexml": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "LSNepomuceno\\PhpPdfProcessor\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php",
            "src/Helpers/fpdfhelpers.php",
            "src/Helpers/mime.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "LSNepomuceno\\PhpPdfProcessor\\Tests\\": "./tests"
        }
    },
    "suggest": {
        "ext-gd": "To use GD library based image processing.",
        "ext-imagick": "To use Imagick based image processing.",
        "lsnepomuceno/laravel-brazilian-ceps": "A package for querying zip codes for Brazilian addresses.",
        "lsnepomuceno/laravel-a1-pdf-sign": "Sign PDF files with valid x509 certificates."
    },
    "minimum-stability": "dev"
}
